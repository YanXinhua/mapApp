{
  "name": "hdmap",
  "version": "0.5.3",
  "description": "hdmap is a web GIS engine using openlayers 3.X",
  "main": "dist/hdmap.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "start": "npm run build",
    "build": "npm run build:hdmap",
    "build:hdmap": "gulp build --gulpfile ./build/build.js",
    "dev": "npm run build && webpack-dev-server --env development",
    "serve": "npm run test && http-server ./public -o",
    "test": "webpack",
    "unit": "karma start unit/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab:xiaxuanyin/UI-mapcomponent.git"
  },
  "keywords": [
    "map",
    "openlayers3"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "myopenlayers": "git+https://github.com/YanXinhua/myopenlayers.git"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "^3.9.1",
    "gulp-banner": "^0.1.3",
    "gulp-base64": "^0.1.3",
    "gulp-clean": "^0.3.2",
    "gulp-clean-css": "^3.9.0",
    "gulp-concat": "^2.6.1",
    "gulp-notify": "^3.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^3.0.0",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "karma": "^2.0.0",
    "less": "^2.7.3",
    "rollup": "^0.52.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-eslint": "^4.0.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.5"
  }
